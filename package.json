{
  "name": "learn-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "npm run build && next export && touch out/.nojekyll",
    "serve": "npm run export && serve out",
    "deploy": "npm run export && node gh-pages.js"
  },
  "dependencies": {
    "chart.js": "^3.5.0",
    "dayjs": "^1.10.7",
    "eslint-config-next": "^11.0.1",
    "gh-pages": "^3.2.3",
    "next": "^10.0.0",
    "npm-license-crawler": "^0.2.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "serve": "^13.0.2"
  },
  "devDependencies": {
    "@types/react": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "2.3.2",
    "pretty-quick": "^3.1.1",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "typescript": "^4.3.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "yarn run lint:fix"
